package com.nagarro.driver.service;

import org.springframework.stereotype.Service;

import com.nagarro.driver.model.DriverModel;

@Service
public class DriverManagementService {
	
	public DriverModel getDriverDetailByID(String driverId) {
		return new DriverModel("driver-" + System.currentTimeMillis(), "Naveen Jain", "5", "Seden");
	}
		

}
